<idea-plugin>
    <id>com.ypsx.plugin.protox</id>
    <name>ProtoX</name>
    <vendor email="chen_liang@ypshengxian.com" url="http://www.ypsx.com">谊品生鲜</vendor>

    <description><![CDATA[
   谊品生鲜定制插件ProtoX.配合Ostrich使用更香哦<br>
    <em>撒花~~~~~~~~~~~~~~</em>
    ]]></description>

    <!-- please see https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.java</depends>
    <depends>io.protostuff.protostuff-jetbrains-plugin</depends>
    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
<!--        <projectService id="ypsx.member.JavaService" serviceImplementation="com.baomidou.plugin.idea.mybatisx.service.JavaService"/>-->

        <codeInsight.lineMarkerProvider language="JAVA" implementationClass="com.ypsx.plugin.idea.proto.provider.OstrichLineMarkerProvider"/>
        <codeInsight.lineMarkerProvider language="PROTO" implementationClass="com.ypsx.plugin.idea.proto.provider.ProtoLineMarkerProvider"/>

        <gotoSymbolContributor
                implementation="com.ypsx.plugin.idea.proto.gotosymbol.JavaGoToProtoSymbol"/>

    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>
</idea-plugin>